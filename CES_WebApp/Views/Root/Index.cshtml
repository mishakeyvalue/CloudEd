@model IEnumerable<AppUser>
    <div class="bg-primary panel-body"><h4>User Accounts</h4></div>
    <table class="table table-condensed table-bordered">
        <tr><th>ID</th><th>Type</th><th>Name</th><th>Login</th><th>Password</th></tr>
        @if (Model.Count() == 0) {
            <tr><td colspan="5" class="text-center">No User Accounts</td></tr>
        }
        else {
            foreach (AppUser user in Model) {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.GetType()</td>
                    <td>@user.UserName</td>
                    <td>@user.Login</td>
                    <td>@user.Password</td>
                </tr>
            }
        }
    </table>
    <a class="btn btn-primary" asp-action="Create">Create</a>

